{
    "name": "Download One Sheet",
    "description": "Download One Sheet",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "https://docs.google.com/*"
            ],
            "js": [
                "index.js"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "icons/32.png",
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs"
    ],
    "icons": {
        "32": "icons/32.png",
        "64": "icons/64.png",
        "128": "icons/128.png"
    },
    "manifest_version": 3
}